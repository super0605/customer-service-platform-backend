{
  "name": "back",
  "private": true,
  "version": "1.0.0",
  "description": "Customer Service Platform RESTful API with Node.js, Express, and PostgreSQL",
  "main": "index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "npm run gen:tsoa && echo TS compiling ... && ttsc -p tsconfig.build.json && echo TS compiled",
    "postbuild": "copyfiles migrations/* build/dist",
    "build:test": "npm run gen:tsoa:test && echo Test TS compiling ... && ttsc -p tsconfig.test.json && echo Test TS compiled",
    "postbuild:test": "copyfiles migrations/* build/test/dist && copyfiles test/migrations/__sql__/* build/test/dist && copyfiles test/setup/__setup-postgres__/* build/test/dist",
    "start": "node build/dist/src/cmds/main.js",
    "start:dev": "ts-node-dev -C ttypescript --inspect -- src/cmds/main.ts",
    "gen:tsoa": "ts-node -C ttypescript src/cmds/generateTsoa.ts",
    "gen:tsoa:test": "ts-node -C ttypescript src/cmds/generateTestTsoa.ts",
    "lint": "eslint src/**/*.ts test/**/*.ts --max-warnings 0",
    "test": "jest",
    "_prepush": "npm run lint && npm run build && npm run build:test && npm run test",
    "prepush": "cross-env EXPLICIT_TSOA_BUILD=true TEST_SUITE=all TEST_COMPILED=true npm run _prepush"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^8.0.1",
    "@sendgrid/mail": "^6.5.1",
    "@types/request": "^2.48.4",
    "aws-sdk": "^2.814.0",
    "camelcase": "^5.3.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-validator": "^6.3.0",
    "fp-ts": "^2.2.0",
    "generate-password": "^1.5.0",
    "helmet": "^3.21.2",
    "http-status-codes": "^1.4.0",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "io-ts": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^0.5.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^7.14.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.0.0",
    "swagger-ui-express": "^4.1.2",
    "tsoa": "^2.5.9",
    "umzug": "^2.2.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.8",
    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mkdirp": "^0.5.2",
    "@types/multer": "^1.3.10",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^12.12.14",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.0",
    "@types/umzug": "^2.2.2",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "@zerollup/ts-transform-paths": "^1.7.8",
    "copyfiles": "^2.1.1",
    "cross-env": "^6.0.3",
    "editorconfig": "^0.15.3",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "factory.ts": "^0.5.0",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "testcontainers": "^2.1.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.8",
    "typescript": "^3.7.2"
  }
}
